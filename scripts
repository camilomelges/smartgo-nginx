docker build -t neuroteks/smart-privacy-home-office-nginx . && docker rm -f smart-privacy-home-office-nginx && docker run -it -p 80:80 --name smart-privacy-home-office-nginx neuroteks/smart-privacy-home-office-nginx

docker build -t neuroteks/smart-privacy-home-office-database ~/projects/neuroteks/smart-privacy-home-office-database/ &&  docker push neuroteks/smart-privacy-home-office-database &&
docker build -t neuroteks/smart-privacy-home-office-api ~/projects/neuroteks/SmartPrivacyHomeOfficeApi/ &&  docker push neuroteks/smart-privacy-home-office-api &&
docker build -t neuroteks/smart-privacy-home-office-manager ~/projects/neuroteks/SmartPrivacyHomeOfficeWeb/ &&  docker push neuroteks/smart-privacy-home-office-manager &&
docker build -t neuroteks/smart-privacy-home-office-employee ~/projects/neuroteks/SmartPrivacyHomeOfficeWebEmployee/ &&  docker push neuroteks/smart-privacy-home-office-employee &&
docker build -t neuroteks/smart-privacy-home-office-nginx ~/projects/neuroteks/SmartPrivacyHomeOfficeNginx/ && docker push neuroteks/smart-privacy-home-office-nginx

docker pull neuroteks/smart-privacy-home-office-database &&
docker pull neuroteks/smart-privacy-home-office-api &&
docker pull neuroteks/smart-privacy-home-office-manager &&
docker pull neuroteks/smart-privacy-home-office-employee &&
docker pull neuroteks/smart-privacy-home-office-nginx

docker network create -d bridge smart-privacy-home-office;

docker run -p 3306:3306 --network smart-privacy-home-office --name smart-privacy-home-office-database -d neuroteks/smart-privacy-home-office-database --default-authentication-plugin=mysql_native_password

docker run -p 5000:5000 --network smart-privacy-home-office --name smart-privacy-home-office-api -d neuroteks/smart-privacy-home-office-api

docker run -p 3000:80 --network smart-privacy-home-office --name smart-privacy-home-office-manager -d neuroteks/smart-privacy-home-office-manager

docker run -p 3001:80 --network smart-privacy-home-office --name smart-privacy-home-office-employee -d neuroteks/smart-privacy-home-office-employee

docker run -it -p 80:80 --network smart-privacy-home-office --name smart-privacy-home-office-nginx -d neuroteks/smart-privacy-home-office-nginx
